{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/products(.*)",
        "dest": "server.js"
      },
      {
        "src": "/api/members(.*)",
        "dest": "server.js"
      },
      {
        "src": "/api/order(.*)",
        "dest": "server.js"
      },
      {
        "src": "/api/auth(.*)",
        "dest": "server.js"
      },
      {
        "src": "/api/payment(.*)",
        "dest": "server.js"
      },
      {
        "src": "/dashboard/(.*)",
        "dest": "server.js"
      }
    ]
  }
  